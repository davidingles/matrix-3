<div class="w-full top-0 left-0 flex fixed bg-white bg-opacity-50 backdrop-blur-sm">
    <div class="navbar flex justify-center items-center border w-full">
        <ul class="flex gap-4">
            <li><a href="/home">Inicio</a></li>
          <li><a href="/about">Acerca de nostros</a></li>
          <li><a href="/contact">Contacto</a></li>
          <li><a href="/contact">Productos</a></li>
        </ul>
    </div>
    <div class="text-center home p-2 w-[3rem] left-0">
        <HomeSvg/>
    </div>
</div>

